<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.hbird.base.mvc.widget.cycleView
            android:id="@+id/imagess"
            android:layout_width="@dimen/width_9_80"
            android:layout_height="@dimen/width_9_80"
            android:layout_gravity="bottom|center"
            android:src="@mipmap/ic_me_normal_headr"
            app:border_color="@color/colorPrimary"
            app:border_width="1dp"
            app:type="circle"/>
        <com.hbird.base.mvc.widget.cycleView
            android:id="@+id/imagess2"
            android:layout_width="@dimen/width_9_80"
            android:layout_height="@dimen/width_9_80"
            android:layout_gravity="bottom|center"
            android:src="@mipmap/ic_me_normal_headr"
            app:border_color="@color/colorYellow"
            app:border_width="1dp"
            app:type="circle"/>
    </FrameLayout>


</LinearLayout>